                  <input .appearance-none .block .w-full .bg-gray-200 .text-gray-700 .border .border-gray-200 .rounded .py-4 .px-4 .leading-tight .focus:outline-none .focus:bg-white .focus:border-gray-500 placeholder="30" type="number" ##{widthField} required>
                 <div .w-full .md:w-1/3 .px-3 .mb-6 .md:mb-0>
                  <label .block .uppercase .tracking-wide .text-gray-700 .text-xs .font-bold .mb-2 for="heightField">
                   Height
                  <input .appearance-none .block .w-full .bg-gray-200 .text-gray-700 .border .border-gray-200 .rounded .py-4 .px-4 .leading-tight .focus:outline-none .focus:bg-white .focus:border-gray-500 placeholder="30" type="number" ##{heightField} required>
                 <div .w-full .md:w-1/3 .px-3 .mb-6 .md:mb-0>
                  <label .block .uppercase .tracking-wide .text-gray-700 .text-xs .font-bold .mb-2 for="bombCountField">
                   Mines
                  <input .appearance-none .block .w-full .bg-gray-200 .text-gray-700 .border .border-gray-200 .rounded .py-4 .px-4 .leading-tight .focus:outline-none .focus:bg-white .focus:border-gray-500 placeholder="150" type="number" ##{bombCountField} required>
                 <div .w-full .px-3 .mb-6 .md:mb-0 .mt-3>
                  <label .block .uppercase .tracking-wide .text-gray-700 .text-xs .font-bold .mb-2 for="randomSeedField">
